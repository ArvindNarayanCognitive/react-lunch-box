{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": ["plugin:react/recommended"],
    "globals": {
        "Atomics": "readable",
        "SharedArrayBuffer": "readable"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "detect"
      }
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}